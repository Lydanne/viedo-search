({
  meta: {
    name: 'clicli',
    home: 'clicli.me',
    version: '202007047',
    search: '超电磁'
  },
  option: {
    delay: 300, //ms
    errRetry: 5, // 重试次数
  },
  steps: [
    {
      url: 'https://api.clicli.us/search/posts?key={{v-meta=search}}',
      resultModel: {
        home: '{{v-meta=home}}',
        id: '{{v-response=data.posts[*].id}}',
        title: '{{v-response=data.posts[*].title}}',
      }
    },
    {
      key:'voides',
      url: 'https://api.clicli.us/videos?pid={{v-prev-mres=[*].id}}&page=1&pageSize=150',
      isMergeResult: false,
      resultModel: {
        name: '第{{v-response=data.videos[*].oid}}集 {{v-response=data.videos[*].title}}',
        url: '{{v-response=data.videos[*].content}}'
      },
    }
  ]
})